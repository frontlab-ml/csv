{
    "compilerOptions": {
        "strict": true,
        "jsx": "react",
        "downlevelIteration": true,
        "sourceMap": true,
        "experimentalDecorators": true,
        "noUnusedLocals": true,
        "noUnusedParameters": true,
        "noUncheckedIndexedAccess": true,
        "allowJs": true,
        "allowSyntheticDefaultImports": true,
        "esModuleInterop": true,
        "forceConsistentCasingInFileNames": true,
        "baseUrl": ".",
        "noEmit": true
    },
}
